# Supabase CLI configuration for MesaYa Webhooks
# Documentation: https://supabase.com/docs/guides/cli/config

[project]
id = "mesaya-webhooks"

[api]
enabled = true
port = 54321
schemas = ["public"]
extra_search_path = ["public", "extensions"]
max_rows = 1000

[db]
port = 54322
major_version = 15

[studio]
enabled = true
port = 54323

[auth]
enabled = false

[storage]
enabled = false

[edge_runtime]
enabled = true
policy = "per_worker"
